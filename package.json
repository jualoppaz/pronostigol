{
    "name": "pronostigol",
    "version": "1.1.2",
    "description": "Aplicación web con estadísticas de distintos sorteos de SELAE.",
    "apidoc": {
        "title": "Documentación de la API Rest de Pronostigol",
        "url": "https://www.pronostigol.es/api"
    },
    "keywords": [
        "Quiniela",
        "Bonoloto",
        "Primitiva",
        "Euromillones",
        "España",
        "Spain"
    ],
    "main": "app.js",
    "scripts": {
        "start": "node app",
        "apidoc": "node node_modules/apidoc/bin/apidoc -i server -o public/docs/",
        "postinstall": "npm run apidoc && node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'bower_components', 'junction') } catch (e) { }\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "webpack --bail --progress --profile",
        "build:dev": "webpack --bail --progress --profile --config webpack-dev.config.js",
        "build:dev:proxy": "webpack --bail --progress --profile --config webpack-dev-proxy.config.js",
        "server:dev": "webpack-dev-server --history-api-fallback --inline --progress --config webpack-dev.config.js --open",
        "server:dev:proxy": "webpack-dev-server --history-api-fallback --inline --progress --config webpack-dev-proxy.config.js --open",
        "server": "webpack-dev-server --history-api-fallback --inline --progress --open"
    },
