                        div.col-md-8.col-sm-12.col-xs-12(style="margin-top: 30px")

                            div.col-xs-12.col-sm-8.col-sm-offset-2.panel.panel-body.border-bonoloto
                                div(style="word-wrap: break-word;")
                                    h2.text-center Índice
