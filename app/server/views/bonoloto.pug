                                            div.fraction
                                                span.fup 5
                                                span.fdn 48
                                            span.multiplication &#x00D7;
                                            div.fraction
                                                span.fup 4
                                                span.fdn 47
                                            span.multiplication &#x00D7;
                                            div.fraction
                                                span.fup 3
                                                span.fdn 46
                                            span.multiplication &#x00D7;
                                            div.fraction
                                                span.fup 2
                                                span.fdn 45
                                            span.multiplication &#x00D7;
                                            div.fraction
                                                span.fup
                                                    del 43
