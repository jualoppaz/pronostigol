extends layout

block head
    title Euromillones | Histórico de sorteos y consulta de estadísticas y resultados de Euromillones
    meta(name="description", content="Euromillones. Histórico de sorteos y consulta de estadísticas y resultados de Euromillones")
    meta(name="keywords", content="estadísticas euromillones, histórico euromillones, resultados euromillones, euromillions")
    link(rel="canonical", href="http://www.pronostigol.es/euromillones")
    link(rel="stylesheet", href="/css/pronostigol.css")


