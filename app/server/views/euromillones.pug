                                                    li
                                                        a.qdb-black(href="#probability-two-numbers-and-one-star", title="Probabilidad de acertar 2 números y 1 estrella en Euromillones") 🔸 Probabilidad de acertar 2 números y 1 estrella en Euromillones
                                                    li
                                                        a.qdb-black(href="#probability-two-numbers-and-two-stars", title="Probabilidad de acertar 2 números y las 2 estrellas en Euromillones") 🔸 Probabilidad de acertar 2 números y las 2 estrellas en Euromillones
                                                    li
