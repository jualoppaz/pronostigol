                                            span.multiplication &#x00D7;
                                            div.combination
                                                | C
                                                span.combination-indexes
                                                    sup 2
                                                    sub 2
                                            span.multiplication &#x00D7;
                                            div.fraction
                                                span.fup 35758800
                                                span.fdn 33561158400
                                            span.equal.
                                                =
                                            span 5
                                            span.multiplication &#x00D7;
                                            span 1
                                            span.multiplication &#x00D7;
                                            div.fraction
                                                span.fup 35758800
                                                span.fdn 33561158400
                                            span.equal.
                                                =
                                            div.fraction
                                                span.fup 178794000
                                                span.fdn 33561158400
                                            span.equal.
