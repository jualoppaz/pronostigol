                include quiniela/partials/navbar

                div(style="padding: 0 0 0 0")

                    main.container-fluid
                        div.logo-parent.col-md-12.text-center
                            div.col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-12.logo.logo-quiniela-con-texto-invertido
                                h1.titulo-logo-juego Quiniela

                        div.col-md-2.col-sm-12.col-xs-12(style="padding-top: 30px; padding-bottom: 30px")
                            script(async, src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js")
